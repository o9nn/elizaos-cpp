{
  "name": "@elizaos/otaku",
  "version": "1.0.0",
  "description": "Otaku is a DeFi-focused ElizaOS agent powered by custom bootstrap plugin and custom build of the ElizaOS server",
  "type": "module",
  "main": "dist/src/index.js",
  "imports": {
    "@/managers/*": "./dist/managers/*.js",
    "@/constants/*": "./dist/constants/*.js"
  },
  "scripts": {
    "dev": "turbo run build && bun run start-server.ts",
    "dev:watch": "turbo run dev --parallel",
    "start": "bun run start-server.ts",
    "build": "turbo run build && bun run build:backend && bun run build:frontend",
    "build:all": "turbo run build",
    "build:backend": "bun run build.ts",
    "build:frontend": "vite build",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .eliza node_modules dist .turbo bun.lockb && find src -type d -name node_modules -exec rm -rf {} + 2>/dev/null || true && find src -type d -name dist -exec rm -rf {} + 2>/dev/null || true"
  },
  "packageManager": "bun@1.2.21",
  "workspaces": [
    "src/plugins/*",
    "src/packages/*"
  ],
  "dependencies": {
    "@coinbase/cdp-core": "0.0.70",
    "@coinbase/cdp-hooks": "0.0.70",
    "@coinbase/cdp-react": "0.0.70",
    "@coinbase/cdp-sdk": "1.38.6",
    "@coinbase/x402": "0.7.3",
    "@elizaos/api-client": "workspace:*",
    "@elizaos/core": "1.6.4",
    "@elizaos/plugin-analytics": "1.0.1",
    "@elizaos/plugin-anthropic": "1.5.12",
    "@elizaos/plugin-bootstrap": "workspace:*",
    "@elizaos/plugin-mcp": "1.3.5",
    "@elizaos/plugin-openai": "1.5.18",
    "@elizaos/plugin-openrouter": "1.5.15",
    "@elizaos/plugin-sql": "1.6.4",
    "@elizaos/server": "workspace:*",
    "@morpho-org/blue-sdk": "4.13.1",
    "@morpho-org/blue-sdk-viem": "3.2.0",
    "@morpho-org/morpho-ts": "2.4.5",
    "@radix-ui/react-accordion": "1.2.2",
    "@radix-ui/react-avatar": "1.1.2",
    "@radix-ui/react-checkbox": "1.1.3",
    "@radix-ui/react-collapsible": "1.1.12",
